# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=devlog
DB_PASSWORD=your_password
DB_NAME=devlog
DB_SSLMODE=disable

# Server Configuration
SERVER_PORT=8080
GIN_MODE=debug
# SSL Configuration (JKS)
SERVER_SSL=false
SERVER_JKS_PATH=JKS/blog.ubanillx.com.jks
SERVER_JKS_PASSWORD=123456

# AI Configuration
# Provider: "openai", "gemini", "ollama", "dashscope" (阿里云百炼)
AI_PROVIDER=dashscope
AI_API_KEY=your_api_key_here
AI_MODEL=qwen-turbo
# 可选模型: qwen-turbo, qwen-plus, qwen-max
# AI_BASE_URL=http://localhost:11434  # For Ollama

# JWT Secret (change in production!)
JWT_SECRET=your-secret-key-change-in-production

# Alibaba Cloud OSS Configuration
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
OSS_ACCESS_KEY_ID=
OSS_ACCESS_KEY_SECRET=
OSS_BUCKET_NAME=
OSS_BASE_URL=

# SEO - Search Engine URL Push Configuration
SEO_SITE_URL=https://your-domain.com
# 百度站长平台
SEO_BAIDU_SITE=your-domain.com
SEO_BAIDU_TOKEN=your-baidu-token
# Bing IndexNow API Key (需在网站根目录放置 {key}.txt 验证文件)
SEO_BING_API_KEY=
# 推送间隔 (默认24小时)
SEO_PUSH_INTERVAL=24h
